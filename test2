pentoo "testing sets"
Choice = 1
Set s
while Choice != 0 do
    pentoo "1: append to set, 2: delete from set, 0: exit"
    lookin Choice
    if Choice == 1 then
        lookin elem
        s.put(elem)
    end
    if Choice == 2 then
        lookin elem
        s.delete(elem)
    end
    pentoo s
end